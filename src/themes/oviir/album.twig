<!DOCTYPE html>
<html lang="en">
<head>

  {% include 'parts/header.twig' %}


</head>

<body>
  <!--/ Nav Start /-->
  {% include 'parts/menu.twig' %}
  <!--/ Nav End /-->

  <!--/ Intro Skew Star /-->
  {% include 'parts/intro.twig' with { 'breadcrumbs': [{ url: base_url, title: 'Avaleht' }, { url: pages['pildid'].url, title: pages['pildid'].meta.title }] } %}
  <!--/ Intro Skew End /-->

  <!--/ Section Blog-Single Start /-->
  <section class="blog-wrapper sect-pt4" id="events">
    <div class="container">
      <div class="row" id="gallery">

      </div>
    </div>
  </section>
  <!--/ Section Blog-Single End /-->

  {% set tools = [] %}

  {% if user_has_right('editor/admin') %}
  {% set tools = tools|merge([
    { title: 'Muuda', type: 'edit', icon: 'icon ion-md-create', id: current_page.id }
  ]) %}
  {% endif %}

  {% include 'parts/footer.twig' with { tools: tools } %}

  {% include 'parts/photoswipe.twig' %}

  {% include 'parts/scripts.twig' %}

  <script type="text/javascript">
    const config = {
      url:'{{ config.miuview.url }}',
      album: '{{ meta.miuview.album }}',
      key: '{{ config.miuview.key }}'
    };
  </script>

  <script src="{{ theme_url }}/{{ jspath }}/miuview.js"></script>
  <script src="{{ theme_url }}/{{ jspath }}/album.js"></script>

</body>
</html>
